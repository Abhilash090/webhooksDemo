                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_if_PermissionSet_s_Group_Needs_to_be_Cloned</targetReference>
            </connector>
